{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev:build-server": "webpack --config webpack.server.dev.js --watch",
    "dev:build-client": "webpack --config webpack.client.dev.js --watch",
    "prod:build-server": "webpack --config webpack.server.prod.js",
    "prod:build-client": "webpack --config webpack.client.prod.js",
    "start": "node build/bundle.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "0.21.1",
    "compression": "1.7.4",
    "concurrently": "4.1.2",
    "core-js": "^3.2.1",
    "express": "4.17.1",
    "express-http-proxy": "1.5.1",
    "lodash": "4.17.15",
    "nodemon": "1.19.2",
    "npm-run-all": "4.1.5",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-helmet": "5.2.1",
    "react-redux": "7.1.1",
    "react-router-config": "5.0.1",
    "react-router-dom": "5.0.1",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "serialize-javascript": "2.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.6.0",
    "@babel/core": "7.6.0",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "7.6.0",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "8.0.6",
    "babel-preset-env": "1.7.0",
    "terser-webpack-plugin": "^2.1.0",
    "webpack": "4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "3.8.1",
    "webpack-merge": "4.2.2",
    "webpack-node-externals": "1.7.2"
  }
}
